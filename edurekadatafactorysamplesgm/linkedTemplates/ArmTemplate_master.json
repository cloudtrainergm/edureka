{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "edurekadatafactorysamplesgm"
		},
		"destADLSGEN2LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'destADLSGEN2LS'"
		},
		"srcADLService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'srcADLService'"
		},
		"srcBLOBStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srcBLOBStorageLS'"
		},
		"srcSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srcSQLDB'"
		},
		"destADLSGEN2LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://edurekadlsdemogm.dfs.core.windows.net/"
		},
		"srcADLService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://edurekadlsdemogm.dfs.core.windows.net/"
		},
		"srcSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "edurekamainserver.database.windows.net"
		},
		"srcSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureworksLT"
		},
		"srcSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "edurekadatafactorysamplesgm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"destADLSGEN2LS_accountKey": {
						"value": "[parameters('destADLSGEN2LS_accountKey')]"
					},
					"srcADLService_accountKey": {
						"value": "[parameters('srcADLService_accountKey')]"
					},
					"srcBLOBStorageLS_connectionString": {
						"value": "[parameters('srcBLOBStorageLS_connectionString')]"
					},
					"srcSQLDB_password": {
						"value": "[parameters('srcSQLDB_password')]"
					},
					"destADLSGEN2LS_properties_typeProperties_url": {
						"value": "[parameters('destADLSGEN2LS_properties_typeProperties_url')]"
					},
					"srcADLService_properties_typeProperties_url": {
						"value": "[parameters('srcADLService_properties_typeProperties_url')]"
					},
					"srcSQLDB_properties_typeProperties_server": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_server')]"
					},
					"srcSQLDB_properties_typeProperties_database": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_database')]"
					},
					"srcSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('srcSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}